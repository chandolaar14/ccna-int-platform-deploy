{
  "projectName": "ccna-int-ccna",
  "production": false,
  "featureFlags": [
    "gatewaySchedulerEvents",
    "enableCloudwatchDetailLogging",
    "adHocHolds"
  ],
  "region": "us-west-2",
  "drRegion": "us-east-1",
  "tfState": {
    "bucket": "tfstate.ccna-int.ccna.info",
    "region": "us-west-2"
  },
  "testDataBucket": "test-data.ccna-int.ccna.info",
  "applicationCode": "INT",
  "applicationId": "UID000015224773",
  "cloudFront": {
    "generate": false
  },
  "auroraScaling": {
    "autopause": true,
    "minCapacity": 4,
    "maxCapacity": 8,
    "secondsUntilAutopause": 7200,
    "timeoutAction": "ForceApplyCapacityChange"
  },
  "cognitoUserPool": {
    "generate": false
  },
  "cloudwatchAlarms": {
    "AWS/DynamoDB": {
      "ThrottledRequests": {
        "statistic": "Sum",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 1,
        "period": 60,
        "evaluationPeriods": 1
      },
      "ReplicationLatency": {
        "statistic": "Average",
        "operator": "GreaterThanThreshold",
        "threshold": 4000,
        "period": 60,
        "evaluationPeriods": 2
      },
      "SystemErrors": {
        "statistic": "Sum",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 1,
        "period": 60,
        "evaluationPeriods": 1
      }
    },
    "AWS/Lambda": {
      "Errors": {
        "statistic": "Sum",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 1,
        "period": 60,
        "evaluationPeriods": 1
      },
      "Throttles": {
        "statistic": "Sum",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 1,
        "period": 60,
        "evaluationPeriods": 1
      },
      "IteratorAge": {
        "statistic": "Average",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 30000,
        "period": 60,
        "evaluationPeriods": 3
      }
    },
    "AWS/SQS": {
      "ApproximateAgeOfOldestMessage": {
        "statistic": "Average",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 600,
        "period": 60,
        "evaluationPeriods": 2
      },
      "ApproximateNumberOfMessagesVisible": {
        "statistic": "Average",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 300,
        "period": 60,
        "evaluationPeriods": 2
      },
      "NumberOfMessagesSent": {
        "statistic": "Sum",
        "operator": "GreaterThanOrEqualToThreshold",
        "threshold": 40000,
        "period": 60,
        "evaluationPeriods": 1
      }
    }
  }
}
